											"referenceName": "ds_adls_source",
											"type": "DatasetReference",
											"parameters": {}
										},
										"logStorageSettings": {
											"linkedServiceName": {
												"referenceName": "ls_nvers_adls",
												"type": "LinkedServiceReference"
											},
											"path": "adf-loging"
										},
										"enableLogging": true,
										"storeSettings": {
											"type": "AzureBlobFSReadSettings",
											"recursive": true,
											"wildcardFileName": "*.parquet",
											"enablePartitionDiscovery": false
										}
									}
								}
